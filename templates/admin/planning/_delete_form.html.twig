<form method="post" action="{{ path('app_admin_planning_delete', {'id': planning.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ces dates ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ planning.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
